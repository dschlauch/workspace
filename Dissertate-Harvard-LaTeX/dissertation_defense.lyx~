\begin_deeper
\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{itemize} 
\end_layout

\begin_layout Plain Layout

		
\backslash
item Individuals may be too similar (due to cryptic relatedness) or too
 different (due to population structure).
\end_layout

\begin_layout Plain Layout

		
\backslash
item Many methods exist for addressing some of these concerns (e.g.
 PCA, LMM).
\end_layout

\begin_layout Plain Layout

		
\backslash
item Localized effects and rare variants increase confounding.
\end_layout

\begin_layout Plain Layout

	
\backslash
end{itemize}
\end_layout

\begin_layout Plain Layout


\backslash
begin{center}
\end_layout

\end_inset


