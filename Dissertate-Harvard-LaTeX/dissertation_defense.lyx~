\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
note[itemize]{ 
\end_layout

\begin_layout Plain Layout

		
\backslash
item Measurements of gene expression are at the mRNA level.
 
\end_layout

\begin_layout Plain Layout

		
\backslash
item Measurements only consist of mRNA abundance.
 
\end_layout

\begin_layout Plain Layout

		
\backslash
item Experimental data is collected as static snapshots.
 
\end_layout

\begin_layout Plain Layout

		
\backslash
item Biological variability can be difficult to induce
\end_layout

\begin_layout Plain Layout

		
\backslash
item   
\end_layout

\begin_layout Plain Layout

		
\backslash
item Gene expression measurements are noisy.
 
\end_layout

\begin_layout Plain Layout

		
\backslash
item Model complexity may require the estimate of too many model parameters.
 
\end_layout

\begin_layout Plain Layout

		
\backslash
item May be computationally intractable.
 
\end_layout

\begin_layout Plain Layout

		
\backslash
item May be statistically undetermined.
 ``The curse of dimensionality''
\end_layout

\begin_layout Plain Layout

}
\end_layout

\end_inset


\end_layout

