\begin_layout Subsection
Background
\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Background
\end_layout

\end_inset


\end_layout

\begin_layout Frame
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
tiny{
\end_layout

\begin_layout Plain Layout


\backslash
begin{itemize} 
\end_layout

\begin_layout Plain Layout

		
\backslash
item Individuals may be too similar (due to cryptic relatedness) or too
 different (due to population structure).
\end_layout

\begin_layout Plain Layout

		
\backslash
item Both features may lead to spurious results, inflation of type I error.
\end_layout

\begin_layout Plain Layout

		
\backslash
item Many methods exist for addressing some of these concerns (e.g.
 PCA, LMM).
\end_layout

\begin_layout Plain Layout

		
\backslash
item Limitations exist, such as with rare alleles and sharp localized effects,
 or with the assumption of linear or discrete population structure.
\end_layout

\begin_layout Plain Layout

	
\backslash
end{itemize}
\end_layout

\begin_layout Plain Layout


\backslash
begin{center}
\end_layout

\end_inset


\end_layout

\begin_layout Frame
\begin_inset Graphics
	filename ../Slides/pasted2.png
	width 40page%

\end_inset


\end_layout

\begin_layout Frame
\begin_inset ERT
status open

\begin_layout Plain Layout

{
\backslash
tiny Mathieson, Nature Genetics 2012}
\backslash
end{center}}
\end_layout

\begin_layout Plain Layout


\backslash
tiny{
\end_layout

\begin_layout Plain Layout

We want to create a similarity measure that...
\end_layout

\begin_layout Plain Layout


\backslash
begin{itemize} 
\end_layout

\begin_layout Plain Layout

		
\backslash
item is more sensitive to fine scale population stratification
\end_layout

\begin_layout Plain Layout

		
\backslash
item can be used as a formal test for cryptic relatedness
\end_layout

\begin_layout Plain Layout

		
\backslash
item can be used as a formal test for population structure 
\end_layout

\begin_layout Plain Layout

	
\backslash
end{itemize}}
\end_layout

\end_inset


\end_layout

\begin_layout Frame

\end_layout

\begin_layout Frame

\end_layout

\begin_layout Frame

\end_layout

\begin_layout Standard
\begin_inset Separator parbreak
\end_inset


\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Basis for measure
\end_layout

\end_inset


\end_layout

\begin_layout Frame

\end_layout

\begin_layout Frame
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
small{
\end_layout

\begin_layout Plain Layout

	
\backslash
begin{itemize} 
\end_layout

\begin_layout Plain Layout

		
\backslash
item Rare variants are recent variants.
\end_layout

\begin_layout Plain Layout

		
\backslash
item In the absense of selection, rare variants become fixed at 0
\backslash
% with high probability over a relatively short timeframe.
\end_layout

\begin_layout Plain Layout

	
\backslash
end{itemize}
\end_layout

\begin_layout Plain Layout

}
\end_layout

\begin_layout Plain Layout


\backslash
begin{center}
\end_layout

\end_inset


\end_layout

