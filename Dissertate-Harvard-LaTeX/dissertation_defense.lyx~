\begin_deeper
\begin_layout Standard
Direct Evidence:
\begin_inset Formula 
\[
d_{i,j}=cor\left(g_{i},g_{j}|\left\{ g_{k,-i}:k\ne i,k\in\mathbf{TF}\right\} \right)^{2}
\]

\end_inset

Indirect Evidence:
\begin_inset Formula 
\[
logit\left(E\left[M_{i}\right]\right)=\beta_{0}+\beta_{1}g_{\left(1\right)}+\dots+\beta_{N}g_{\left(n\right)}
\]

\end_inset


\begin_inset Formula 
\[
e_{i,j}=\frac{1}{1+e^{\beta_{0}+\beta_{1}g_{j,\left(1\right)}+\dots+\beta_{k}g_{j,\left(k\right)}}}
\]

\end_inset

Edgeweight:
\begin_inset Formula 
\[
w_{i,j}=\left(1-\alpha\right)\left[d_{i,j}\right]+\alpha\left[e_{i,j}\right]
\]

\end_inset


\end_layout

\end_deeper
\begin_layout Frame

\end_layout

\begin_layout Standard
\begin_inset Separator parbreak
\end_inset


\end_layout

\begin_layout Subsection
Network Transition
\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Network Transition
\end_layout

\end_inset


\begin_inset Separator latexpar
\end_inset


\end_layout

\begin_deeper
\begin_layout Standard
\begin_inset Graphics
	filename transition_slide_1.pdf
	width 100col%

\end_inset


\end_layout

\end_deeper
\begin_layout Standard
\begin_inset Separator parbreak
\end_inset


\end_layout

