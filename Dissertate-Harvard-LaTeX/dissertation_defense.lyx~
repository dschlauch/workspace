\begin_layout Subsection
Similarity measure
\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Test Statistic
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
center{STEGO: Similarity Test for Estimating Genetic Outliers}
\end_layout

\end_inset


\begin_inset Formula 
\[
s_{i,j}=\frac{\sum_{k=1}^{N}w_{k}\mathbf{G}_{i,k}\mathbf{G}_{j,k}}{\sum_{k=1}^{N}I\left[\sum_{l=1}^{2n}\mathbf{G}_{l,k}>1\right]}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
flushleft{where }
\end_layout

\end_inset


\end_layout

\begin_layout Standard

\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none
\begin_inset Formula 
\[
w_{k}=\begin{cases}
\frac{{2n \choose 2}}{{\sum_{l=1}^{2n}\mathbf{G}_{l,k} \choose 2}} & \sum_{l=1}^{2n}\mathbf{G}_{l,k}>1\\
0 & \sum_{l=1}^{2n}\mathbf{G}_{l,k}\le1
\end{cases}
\]

\end_inset


\end_layout

\begin_layout Standard
Under 
\begin_inset Formula $H_{0}$
\end_inset

, homogeneity, 
\begin_inset Formula 
\[
s_{i,j}\sim N\left(1,\sigma_{i,j}^{2}\right)
\]

\end_inset


\end_layout

\end_deeper
\begin_layout Standard
\begin_inset Separator parbreak
\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
note[itemize]{ 
\end_layout

\begin_layout Plain Layout

		
\backslash
item Distribution under the null has expectation 1, and variance is defined
 ONLY by the allele frequencies
\end_layout

\begin_layout Plain Layout

}
\end_layout

\end_inset


\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Tests of Heterogeneity
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Standard
\begin_inset Formula 
\[
H_{0}:E\left[s_{i,j}\right]=1\forall i,j\in1\dots n
\]

\end_inset


\begin_inset Formula 
\[
H_{A}:\exists i,j\in1\dots n\mid E\left[s_{i,j}\right]\ne1
\]

\end_inset


\end_layout

\begin_layout Standard
Test for cryptic relatedness:
\begin_inset Formula 
\[
R:max\left(s_{i,j}\right)>1-probit\left(\frac{\alpha}{{n \choose 2}}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
Test for population structure:
\begin_inset Formula 
\[
K=\sup_{x}\left|F_{s}\left(x\right)-\Phi\left(x\right)\right|
\]

\end_inset


\end_layout

\end_deeper
\begin_layout Standard
\begin_inset Separator parbreak
\end_inset


\end_layout

