\begin_layout Plain Layout

		$$f
\backslash
left[Gene2|BatchA
\backslash
right]=f
\backslash
left[Gene2|BatchB
\backslash
right]$$
\end_layout

\begin_layout Plain Layout

		$$f
\backslash
left[Gene1,Gene2|BatchA
\backslash
right]
\backslash
ne f
\backslash
left[Gene1,Gene2|BatchB
\backslash
right]$$
