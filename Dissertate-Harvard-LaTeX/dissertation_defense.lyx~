\begin_layout Subsection
Similarity measure
\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Test Statistic
\end_layout

\end_inset


\end_layout

\begin_layout Frame

\end_layout

\begin_layout Frame
\begin_inset Formula 
\[
s_{i,j}=\frac{\sum_{k=1}^{N}w_{k}\mathbf{G}_{i,k}\mathbf{G}_{j,k}}{\sum_{k=1}^{N}I\left[\sum_{l=1}^{2n}\mathbf{G}_{l,k}>1\right]}
\]

\end_inset


\end_layout

\begin_layout Frame
where 
\end_layout

\begin_layout Frame

\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none
\begin_inset Formula 
\[
w_{k}=\begin{cases}
\frac{{2n \choose 2}}{{\sum_{l=1}^{2n}\mathbf{G}_{l,k} \choose 2}} & \sum_{l=1}^{2n}\mathbf{G}_{l,k}>1\\
0 & \sum_{l=1}^{2n}\mathbf{G}_{l,k}\le1
\end{cases}
\]

\end_inset


\end_layout

\begin_layout Frame
\begin_inset Formula 
\begin{align*}
E\left[s_{i,j}\right] & =1
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator parbreak
\end_inset


\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Test Statistic
\end_layout

\end_inset


\end_layout

\begin_layout Frame
In the absence of population structure, cryptic relatedness and dependence
 between loci the distribution of the similarity index, 
\begin_inset Formula $s_{i,j}$
\end_inset

 
\begin_inset Formula 
\[
s_{i,j}\sim N\left(1,\sigma_{i,j}^{2}\right)
\]

\end_inset

Where the variance of 
\begin_inset Formula $s_{ij}$
\end_inset

 can be estimated by 
\begin_inset Formula 
\[
\hat{\sigma_{i,j}^{2}}=\hat{Var}\left(s_{i,j}\right)=\frac{\sum_{k=1}^{N}\left(w_{k}-1\right)}{\left(\sum_{k=1}^{N}I\left[\sum_{l=1}^{2n}\mathbf{G}_{l,k}>1\right]\right)^{2}}
\]

\end_inset


\begin_inset Formula 
\[
s_{i,j}^{\left(diploid\right)}=\frac{\sum_{k=1}^{N}\left[w_{k}\mathbf{H}_{i,k}\mathbf{H}_{j,k}\right]/4}{\sum_{k=1}^{N}I\left[\left(\sum_{l=1}^{n}\mathbf{H}_{l,k}\right)>1\right]}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator parbreak
\end_inset


\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Tests of Heterogeneity
\end_layout

\end_inset


\end_layout

\begin_layout Frame
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
tiny{}
\end_layout

\end_inset


\begin_inset Formula 
\[
\hat{\phi}_{i,j}=\frac{s_{i,j}-1}{\left[\frac{\sum_{k=1}^{N}\hat{p_{k}}w_{k}}{\sum_{k=1}^{N}I\left[\sum_{l=1}^{2n}\mathbf{G}_{l,k}>1\right]}-1\right]}
\]

\end_inset


\end_layout

\begin_layout Frame
\begin_inset Formula 
\[
R:max\left(s_{i,j}\right)>1-probit\left(\frac{\alpha}{{n \choose 2}}\right)
\]

\end_inset


\begin_inset Formula 
\[
P\left(Reject\,H_{0}|\phi_{i,j}=\gamma\right)=\alpha+\left(1-\alpha\right)\left(1-\Phi\left(\frac{\mu_{i,j}-1}{\sqrt{\hat{\sigma^{2}}_{i,j}}}\right)\right)
\]

\end_inset


\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{center}
\end_layout

\end_inset


\begin_inset Graphics
	filename /home/dan/1000GP/plots/powerCurve.pdf
	width 50col%

\end_inset


\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
end{center}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator parbreak
\end_inset


\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Tests of Heterogeneity
\end_layout

\end_inset


\end_layout

\begin_layout Frame
\begin_inset Formula 
\[
H_{0}:\mu_{i,j}=1\forall i,j\in1\dots n
\]

\end_inset


\begin_inset Formula 
\[
H_{A}:\exists i,j\in1\dots n|\mu_{i,j}\ne1
\]

\end_inset


\end_layout

\begin_layout Frame
Test for population structure:
\begin_inset Formula 
\[
K=\sup_{x}\left|F_{s}\left(x\right)-\Phi\left(x\right)\right|
\]

\end_inset


\end_layout

\begin_layout Frame
Test for cryptic relatedness:
\begin_inset Formula 
\[
R:max\left(s_{i,j}\right)>1-probit\left(\frac{\alpha}{{n \choose 2}}\right)
\]

\end_inset


\end_layout

\begin_layout Frame

\end_layout

\begin_layout Standard
\begin_inset Separator parbreak
\end_inset


\end_layout

